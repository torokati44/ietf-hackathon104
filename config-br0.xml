<?xml version="1.0" encoding="UTF-8"?>
<config xmlns="urn:ietf:params:xml:ns:netconf:base:1.0">
  <bridge xmlns="urn:ietf:params:xml:ns:yang:ietf-network-bridge">
    <ports>
      <port>
        <name>p0</name>
      </port>
      <port>
        <name>p1</name>
      </port>
      <port>
        <name>p2</name>
      </port>
    </ports>
  </bridge>
  <flows xmlns="urn:ietf:params:xml:ns:yang:ietf-network-bridge-flows">
    <flow>
      <id>p0-to-p1</id>
      <match>
        <in-port>p0</in-port>
        <ethernet-match>
          <ethernet-source>
            <address>00:00:00:00:00:00</address>
          </ethernet-source>
          <ethernet-destination>
            <address>00:00:00:00:00:01</address>
          </ethernet-destination>
        </ethernet-match>
      </match>
      <actions>
        <action>
          <order>0</order>
          <output-action>
            <out-port>p1</out-port>
          </output-action>
        </action>
      </actions>
    </flow>
    <flow>
      <id>p0-to-p2</id>
      <match>
        <in-port>p0</in-port>
        <ethernet-match>
          <ethernet-source>
            <address>00:00:00:00:00:00</address>
          </ethernet-source>
          <ethernet-destination>
            <address>00:00:00:00:00:02</address>
          </ethernet-destination>
        </ethernet-match>
      </match>
      <actions>
        <action>
          <order>0</order>
          <output-action>
            <out-port>p2</out-port>
          </output-action>
        </action>
      </actions>
    </flow>
    <flow>
      <id>p1-to-p0</id>
      <match>
        <in-port>p1</in-port>
        <ethernet-match>
          <ethernet-source>
            <address>00:00:00:00:00:01</address>
          </ethernet-source>
          <ethernet-destination>
            <address>00:00:00:00:00:00</address>
          </ethernet-destination>
        </ethernet-match>
      </match>
      <actions>
        <action>
          <order>0</order>
          <output-action>
            <out-port>p0</out-port>
          </output-action>
        </action>
      </actions>
    </flow>
    <flow>
      <id>p1-to-p2</id>
      <match>
        <in-port>p1</in-port>
        <ethernet-match>
          <ethernet-source>
            <address>00:00:00:00:00:01</address>
          </ethernet-source>
          <ethernet-destination>
            <address>00:00:00:00:00:02</address>
          </ethernet-destination>
        </ethernet-match>
      </match>
      <actions>
        <action>
          <order>0</order>
          <output-action>
            <out-port>p2</out-port>
          </output-action>
        </action>
      </actions>
    </flow>
    <flow>
      <id>p2-to-p0</id>
      <match>
        <in-port>p2</in-port>
        <ethernet-match>
          <ethernet-source>
            <address>00:00:00:00:00:02</address>
          </ethernet-source>
          <ethernet-destination>
            <address>00:00:00:00:00:00</address>
          </ethernet-destination>
        </ethernet-match>
      </match>
      <actions>
        <action>
          <order>0</order>
          <output-action>
            <out-port>p0</out-port>
          </output-action>
        </action>
      </actions>
    </flow>
    <flow>
      <id>p2-to-p1</id>
      <match>
        <in-port>p2</in-port>
        <ethernet-match>
          <ethernet-source>
            <address>00:00:00:00:00:02</address>
          </ethernet-source>
          <ethernet-destination>
            <address>00:00:00:00:00:01</address>
          </ethernet-destination>
        </ethernet-match>
      </match>
      <actions>
        <action>
          <order>0</order>
          <output-action>
            <out-port>p1</out-port>
          </output-action>
        </action>
      </actions>
    </flow>
    <flow>
      <id>unmatched-from-p0</id>
      <match>
        <in-port>p0</in-port>
      </match>
      <actions>
        <action>
          <order>0</order>
          <output-action>
            <out-port>p1</out-port>
          </output-action>
        </action>
        <action>
          <order>1</order>
          <output-action>
            <out-port>p2</out-port>
          </output-action>
        </action>
      </actions>
    </flow>
    <flow>
      <id>unmatched-from-p1</id>
      <match>
        <in-port>p1</in-port>
      </match>
      <actions>
        <action>
          <order>0</order>
          <output-action>
            <out-port>p0</out-port>
          </output-action>
        </action>
        <action>
          <order>1</order>
          <output-action>
            <out-port>p2</out-port>
          </output-action>
        </action>
      </actions>
    </flow>
    <flow>
      <id>unmatched-from-p2</id>
      <match>
        <in-port>p2</in-port>
      </match>
      <actions>
        <action>
          <order>0</order>
          <output-action>
            <out-port>p0</out-port>
          </output-action>
        </action>
        <action>
          <order>1</order>
          <output-action>
            <out-port>p1</out-port>
          </output-action>
        </action>
      </actions>
    </flow>
  </flows>
  <interfaces xmlns="urn:ietf:params:xml:ns:yang:ietf-interfaces">
    <interface>
      <name>p0</name>
      <type
        xmlns:ianaift="urn:ietf:params:xml:ns:yang:iana-if-type">ianaift:ethernetCsmacd</type>
      <port-name xmlns="urn:ietf:params:xml:ns:yang:ietf-network-bridge">p0</port-name>
    </interface>
    <interface>
      <name>p1</name>
      <type
        xmlns:ianaift="urn:ietf:params:xml:ns:yang:iana-if-type">ianaift:ethernetCsmacd</type>
      <port-name xmlns="urn:ietf:params:xml:ns:yang:ietf-network-bridge">p1</port-name>
    </interface>
    <interface>
      <name>p2</name>
      <type
        xmlns:ianaift="urn:ietf:params:xml:ns:yang:iana-if-type">ianaift:ethernetCsmacd</type>
      <port-name xmlns="urn:ietf:params:xml:ns:yang:ietf-network-bridge">p2</port-name>
    </interface>
  </interfaces>
  <nacm xmlns="urn:ietf:params:xml:ns:yang:ietf-netconf-acm">
  </nacm>
</config>
